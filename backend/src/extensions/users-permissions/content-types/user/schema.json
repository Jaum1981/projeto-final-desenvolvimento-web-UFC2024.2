{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "doacao": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::doacao.doacao",
      "mappedBy": "doacao"
    },
    "solicitante": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::doacao.doacao",
      "inversedBy": "solicitantes"
    }
  }
}
